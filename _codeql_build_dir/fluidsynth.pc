prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: FluidSynth
Description: Software SoundFont synth
Version: 2.5.3
Requires.private: glib-2.0 gthread-2.0
Libs: -L${libdir} -lfluidsynth
Libs.private: -lm /usr/lib/gcc/x86_64-linux-gnu/13/libgomp.so /usr/lib/x86_64-linux-gnu/libpthread.a
Cflags: -I${includedir}
